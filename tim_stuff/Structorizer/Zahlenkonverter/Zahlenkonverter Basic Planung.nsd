						<instruction text="&#34;valideZahl = true&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
					</qTrue>
					<qFalse>
						<instruction text="&#34;Ausgabe &#34;&#34;nicht valide Zahl&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
					</qFalse>
				</alternative>
			</qWhile>
		</while>
		<instruction text="&#34;ausgabe zahl.toBin&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
		<instruction text="&#34;ausgabe zahl.toHex&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
	</children>
</root>