<root text="&#34;Zahlenkonverter Basic Planung (blalblalblalblblabla brauch langen titel)&#34;" comment="&#34;&#34;" color="ffffff" type="program" style="nice">
	<children>
		<instruction text="&#34;valideZahl = false&#34;" comment="&#34;&#34;" color="ffffff" rotated="0"></instruction>
		<while text="&#34;solange valideZahl = false&#34;" comment="&#34;&#34;" color="ffffff">
			<qWhile>
